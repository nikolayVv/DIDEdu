<!-- Introduction start -->
<div class="about container-fluid pt-5 mt-5">
    <div class="d-flex flex-column justify-content-center pt-4 mt-5 pb-5 mb-5">
        {{{introduction}}}
    </div>
</div>
<!-- Introduction end -->

<!-- About start -->
<div id="aboutSection" class="container-fluid section">
    <div class="d-flex flex-column justify-content-center pb-4">
        <h1 class="pt-4 text-center containerTitle1 text-uppercase">About</h1>
        {{#each abouts as |about|}}
            <div class="row pt-5">
                {{#each about.contents as |content|}}
                    <div class="col-md-3 col-lg-4 col-xl-3 mx-auto mb-4">
                        <h4 class="fw-bold mb-4 containerText1">{{{content.header}}}</h4>
                        <p>{{content.body}}</p>
                    </div>
                {{/each}}
            </div>
        {{/each}}
    </div>
</div>
<!-- About end -->

<!-- Tutorial start -->
<div id="tutorialSection" class="tutorial container-fluid section">
    <div class="d-flex flex-column justify-content-center pb-4 pt-3">
        <h1 class="pt-4 pb-2 text-center containerTitle2 text-uppercase">How to</h1>
        <div class="row mt-5">
            {{{videos}}}
        </div>
    </div>
</div>
<!-- Tutorial end -->

<!-- Register start -->
<div id="registerSection" class="container-fluid section">
    <div class="d-flex flex-column justify-content-center pb-5">
        <h1 class="pt-4 text-center containerTitle1 text-uppercase">Get started</h1>
    </div>
    <div class="registerForm row container-fluid bg-light mb-5 pe-0 mt-3">
        <div class="col col-lg-8 col-xl-8 col-xxl-9 pt-4">
            <h2>{{{registerTitle}}}</h2>
            <h4 class="containerText1 pt-1">{{registerSubTitle}}</h4>
            <p class="registerText pt-4">{{{registerContent}}}</p>
            <div class="row pt-4 pb-3">
                <div class="col d-flex justify-content-end pt-5 containerText1">
                    {{{registerBell}}}
                </div>
                <div class="col d-flex justify-content-center">
                    {{{registerButton}}}
                </div>
                <div class="col d-flex justify-content-start pt-5 containerText1">
                    {{{registerBell}}}
                </div>
            </div>
        </div>
        <div class="col col-lg-4 col-xl-4 col-xxl-3 pe-0 d-sm-none d-none d-md-block d-flex justify-content-center">
            <div class="register h-100">
                <div class="registerBox w-100 pt-2">
                    <h2>{{{registerPhone}}}</h2>
                </div>
            </div>
        </div>

    </div>
</div>
<!-- Register end -->

<!-- FAQ start -->
<div id="faqSection" class="container-fluid bg-light section">
    <div class="d-flex flex-column justify-content-center pb-4">
        <h1 class="pt-4 text-center containerTitle1 text-uppercase">Most asked</h1>
        {{#each questions as |question|}}
            <div class="row pt-5">
            {{#each question.contents as |content|}}

                    <div class="col-md-3 col-lg-3 col-xl-3 mx-auto mb-4">
                        <h5 class="question pb-2">{{{content.title}}}</h5>
                        <p>{{content.body}}</p>
                    </div>
            {{/each}}
            </div>
        {{/each}}
    </div>
</div>
<!-- FAQ end -->

<!-- Contact start -->
<div id="contactSection" class="container-fluid section">
    <div class="d-flex flex-column justify-content-center pb-4">
        <h1 class="pt-4 text-center containerTitle1 text-uppercase">Contact us</h1>
    </div>
    <div class="row pt-3 pb-2">
        <div class="col col-xs-12 col-sm-12 col-lg-6 col-xl-7">
            <form action="#" class="pb-5 ps-3">
                {{#each inputGroups as |inputGroup|}}
                     <div class="input-group mb-3 row">
                        {{{inputs inputGroup.inputs}}}
                     </div>
                {{/each}}
                <button type="submit" class="btn mainBtn mt-3">Send</button>
            </form>
        </div>
        <div class="col col-lg-4 col-xl-5 d-flex justify-content-center pt-5 mt-4 d-sm-none d-none d-md-none d-lg-block ps-5">
            {{{sendIcon}}}
        </div>
    </div>
</div>
<!-- Contact end -->

